#!/usr/bin/env swipl


% Super irritating style check
:- style_check(-atom).

:- ensure_loaded('../balance/balancer').


% Meta error handling
:- ensure_loaded('../io/meta_error').

:- use_module(units).
:- use_module(calculate).
:- use_module(quantity).


name_quantity(Sym,Rest,Formula,Qty) --> quantity(Qty), " ", name(Sym,Rest,Formula).
formula_quantity(Qty,Fmt,Elems,ElemsR,Formula,FormulaR,Qty) --> quantity(Qty), " ", formula(Fmt,Elems,ElemsR,Formula,FormulaR).



% vi: ft=prolog
